{
  "build": {
    "arduino":{
      "ldscript": "linker_script.ld"
    },
    "core": "arduino",
    "cpu": "cortex-m4",
    "extra_flags": "-DARDUINO_SEEED_XIAO_SENSE -DARDUINO_ARCH_NRF52840",
    "f_cpu": "64000000L",
    "mcu": "nrf52840",
    "variant": "SEEEDSTUDIO_XIAO"
  },
  "connectivity": [
    "bluetooth"
  ],
  "debug": {
    "jlink_device": "nRF52840_xxAA",
    "openocd_target": "nrf52.cfg",
    "svd_path": "nrf52840.svd",
    "onboard_tools": [
      "cmsis-dap"
    ]
  },
  "frameworks": [
    "arduino"
  ],
  "name": "Seeed Studio XIAO nRF52840 (Sense)",
  "upload": {
    "maximum_ram_size": 262144,
    "maximum_size": 2097152,
    "protocol": "cmsis-dap",
    "protocols": [
      "jlink",
      "nrfjprog",
      "nrfutil",
      "cmsis-dap",
      "blackmagic"
    ],
    "use_1200bps_touch": true,
    "require_upload_port": true,
    "wait_for_upload_port": true
  },
  "url": "https://www.seeedstudio.com/Seeed-XIAO-BLE-nRF52840-p-5201.html",
  "vendor": "Seeed Studio"
}
